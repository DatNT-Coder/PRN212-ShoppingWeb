@model IEnumerable<OrderDetails>

@{
    ViewData["title"] = "Order Details";



}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h3>List Order</h3>



    <table class="table" id="myTable">
        <thead>
            <tr>
                <th>Id</th>
                <th>Order Code</th>
                <th>Shipping Cost</th>
                <th>Coupon</th>
                <th>UserName</th>
                <th>Order Status</th>
                <th>Created Date</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.OrderCode</td>
                    
                    
                </tr>
            }
        </tbody>
    </table>

</body>
</html>
